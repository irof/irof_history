<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:nakamap="http://schemas.android.com/apk/res-auto"
    >
    <com.kayac.libnakamap.components.ImageLoaderView
        android:id="@+id/nakamap_framed_image_loader_loader"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        nakamap:nakamap_useMemoryCache="true"
        />
    <ImageView
        android:id="@+id/nakamap_framed_image_loader_frame"
        android:src="@drawable/nakamap_frame_s"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <ImageView 
        android:id="@+id/nakamap_framed_image_loader_change_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/nakamap_ico_photo_profile"
        android:visibility="gone"
        />
</merge>